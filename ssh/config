Include ~/.orbstack/ssh/config

Host github.com
  HostName github.com
  User git
  UseKeychain yes
  AddKeysToAgent yes
  IdentityFile ~/.ssh/hungvx.dev

Host gitlab.com
  HostName gitlab.com
  User git
  UseKeychain yes
  AddKeysToAgent yes
  IdentityFile ~/.ssh/hungvx.dev

Host bitbucket.org
  HostName bitbucket.org
  User git
  IdentitiesOnly yes
  IdentityFile ~/.ssh/hungvx.dev

Host gitlab.com-hf
  HostName gitlab.com
  User git
  UseKeychain yes
  AddKeysToAgent yes
  IdentityFile ~/.ssh/hung.vx.hodfords

