  HostName gitlab.com
  User git
  UseKeychain yes
  AddKeysToAgent yes
  IdentitiesOnly yes
  IdentityFile ~/.ssh/hungvx.dev

Host gitlab.com-hf
  HostName gitlab.com
  User git
  UseKeychain yes
  AddKeysToAgent yes
  IdentitiesOnly yes
  IdentityFile ~/.ssh/hungvx.hodfords

Host digital-ubuntu
