Include ~/.orbstack/ssh/config

Host github.com
  HostName github.com
  User git
  UseKeychain yes
  AddKeysToAgent yes
  IdentityFile ~/.ssh/hungvx.dev

Host gitlab.com
  HostName gitlab.com
  User git
  UseKeychain yes
  AddKeysToAgent yes
  IdentityFile ~/.ssh/hungvx.dev

Host bitbucket.org
  HostName bitbucket.org
  User git
  IdentitiesOnly yes
  IdentityFile ~/.ssh/hungvx.dev

Host gitlab.com-hf
  HostName gitlab.com
  PreferredAuthentications publickey
  UseKeychain yes
  AddKeysToAgent yes
  IdentitiesOnly yes
  IdentityFile ~/.ssh/hungvx.hodfords

Host digital-ubuntu
  HostName 157.230.41.226
  User root
  AddKeysToAgent yes
  IdentitiesOnly yes
  IdentityFile ~/.ssh/digitalocean.learndevops.ubuntu
